{
	"id": "/docker-registry",
	"cmd": "(docker rm $(docker ps -aq -f status=exited) || echo 'Processing...') && docker load --input registry_2.tar && docker run -p $PORT0:5000 --restart=always --name registry registry:2",
	"cpu": "1",
	"mem": 1024,
	"instances": 1,
	"fetch": [{
		"uri": "http://${S3_ENDPOINT}/${ARTIFACTS_S3_BUCKET}/packages/docker-tars/registry.tar",
		"executable": false,
		"extract": false,
		"cache": false
	}],
	"portDefinitions": [
    		{
      			"protocol": "tcp",
      			"port": 10005,
      			"labels": {
        			"VIP_0": "/docker-registry:10005"
      			}
    		}
  	],
  	"requirePorts": true
}
